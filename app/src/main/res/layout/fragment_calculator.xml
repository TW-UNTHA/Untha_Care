<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_calculator"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackgroundMainRoute"
    tools:context=".view.fragments.CalculatorFragment">


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/calculatorRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:focusable="true"
        android:focusableInTouchMode="true"
        tools:listitem="@layout/layout_calculator_item">

    </androidx.recyclerview.widget.RecyclerView>

</RelativeLayout>
