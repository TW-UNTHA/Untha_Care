<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/colorBackgroundMainRoute"
    tools:context=".view.activities.MainActivity">

    <RelativeLayout
        android:id="@+id/rl_decimo_tercero_annual"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        android:background="@color/colorCalculatorGroupResult"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible">

        <TextView
            android:id="@+id/tv_decimo_tercero_annual_information"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:gravity="left|center_vertical"
            android:text="@string/result_decimo_tercero_annual"
            android:textColor="@color/colorCalculatorText"
            android:textSize="@dimen/text_size_content" />

        <TextView
            android:id="@+id/tv_decimo_tercero_annual"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="25dp"
            android:layout_marginRight="25dp"
            android:layout_marginBottom="4dp"
            android:textColor="@color/colorCalculatorText"
            android:textSize="@dimen/text_size_content" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_decimo_cuarto_annual"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        android:background="@color/colorCalculatorGroupResult"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rl_decimo_tercero_annual"
        app:layout_constraintVertical_bias="0.124"
        tools:visibility="visible">

        <TextView
            android:id="@+id/tv_decimo_cuarto_annual_information"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:gravity="left"
            android:text="@string/result_decimo_cuarto_annual"
            android:textColor="@color/colorCalculatorText"
            android:textSize="@dimen/text_size_content" />

        <TextView
            android:id="@+id/tv_decimo_cuarto_annual"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="25dp"
            android:layout_marginRight="25dp"
            android:layout_marginBottom="4dp"
            android:textColor="@color/colorCalculatorText"
            android:textSize="@dimen/text_size_content" />

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>

